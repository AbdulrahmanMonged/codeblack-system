# API base URL used by frontend requests.
VITE_API_BASE_URL=/api/v1

# Optional explicit app environment label.
VITE_APP_ENV=development

# Frontend-only unlock for local development.
# Effective only when environment is development/dev/local/test.
VITE_DEV_UNLOCK_ALL=false

# Google reCAPTCHA v3 site key for application submission wizard.
VITE_RECAPTCHA_SITE_KEY=

# reCAPTCHA action used by the wizard submit flow.
VITE_RECAPTCHA_ACTION=application_submit
